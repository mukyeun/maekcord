{
  "name": "maekcord-frontend",
  "version": "1.0.0",
  "description": "의료 대기열 관리 시스템 프론트엔드",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.12.8",
    "axios": "^1.6.2",
    "dayjs": "^1.11.13",
    "dompurify": "^3.0.5",
    "framer-motion": "^12.12.2",
    "html2pdf.js": "^0.10.3",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "react-to-print": "^3.1.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.17",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "REACT_APP_ENV=staging react-scripts build",
    "build:production": "REACT_APP_ENV=production react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --ci",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "docs": "jsdoc -c jsdoc.json",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "predeploy": "npm run test:ci && npm run build",
    "deploy": "npm run predeploy",
    "serve": "npx serve -s build -l 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000/",
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/maekcord.git"
  },
  "keywords": [
    "medical",
    "queue",
    "management",
    "react",
    "healthcare"
  ],
  "author": "Maekcord Development Team",
  "license": "MIT",
  "homepage": "."
}
